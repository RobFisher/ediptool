[Icons]
Name: "{group}\ediptool"; Filename: "{app}\ediptool.exe"
Name: "{group}\{cm:UninstallProgram,ediptool}"; Filename: "{uninstallexe}"
Name: "{commondesktop}\ediptool"; Filename: "{app}\ediptool.exe"; Tasks: desktopicon

[Run]
Filename: "{app}\ediptool.exe"; Description: "{cm:LaunchProgram,ediptool}"; Flags: nowait postinstall skipifsilent

