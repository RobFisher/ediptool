[Setup]
; NOTE: The value of AppId uniquely identifies this application.
; Do not use the same AppId value in installers for other applications.
; (To generate a new GUID, click Tools | Generate GUID inside the IDE.)
AppId={{7FB712C3-0679-4B64-B481-C5909A48EFAF}
AppName=ediptool
AppVersion=1.0
;AppVerName=ediptool 1.0
AppPublisher=Rob Fisher
AppPublisherURL=https://github.com/RobFisher/ediptool
AppSupportURL=https://github.com/RobFisher/ediptool
AppUpdatesURL=https://github.com/RobFisher/ediptool
DefaultDirName={pf}\ediptool
DefaultGroupName=Ediptool
AllowNoIcons=yes
OutputBaseFilename=setup
Compression=lzma
SolidCompression=yes

[Languages]
Name: "english"; MessagesFile: "compiler:Default.isl"

[Tasks]
