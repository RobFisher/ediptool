Source: "E:\projects\ediptool\dist\unicodedata.pyd"; DestDir: "{app}"; Flags: ignoreversion
Source: "E:\projects\ediptool\dist\w9xpopen.exe"; DestDir: "{app}"; Flags: ignoreversion
Source: "E:\projects\ediptool\dist\_ctypes.pyd"; DestDir: "{app}"; Flags: ignoreversion
Source: "E:\projects\ediptool\dist\_hashlib.pyd"; DestDir: "{app}"; Flags: ignoreversion
Source: "E:\projects\ediptool\dist\_socket.pyd"; DestDir: "{app}"; Flags: ignoreversion
Source: "E:\projects\ediptool\dist\_ssl.pyd"; DestDir: "{app}"; Flags: ignoreversion
